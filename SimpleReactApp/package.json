{
  "name": "simple-react-app",
  "version": "0.0.0",
  "description": "SimpleReactApp",
  "scripts": {
    "start": "node server.js",
    "build": "webpack",
    "test": "jest",
    "test:coverage": "set CI=true && jest --coverage",
    "build-dev": "webpack --env.NODE_ENV=development",
    "build-prod": "webpack --env.NODE_ENV=production",
    "start-local": "npx webpack-dev-server --env.NODE_ENV=development",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "dependencies": {
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "express": "^4.17.1",
    "jest": "^24.9.0",
    "jss": "^10.0.0",
    "query-string": "^6.9.0",
    "react": "^16.11.0",
    "react-bootstrap": "^1.0.0-beta.15",
    "react-dom": "^16.11.0",
    "react-jss": "^10.0.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-swagger-client": "^2.1.1",
    "redux-thunk": "^2.3.0",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@cypress/code-coverage": "^1.10.2",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "^3.2.0",
    "cypress": "^3.7.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/*.{js,jsx}",
      "**/src/components/**",
      "!**/src/index.js",
      "!**/src/app.js",
      "!**/src/components/netflixTheme.js"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
