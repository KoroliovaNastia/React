{
  "name": "simple-react-app",
  "version": "0.0.0",
  "description": "SimpleReactApp",
  "scripts": {
    "server-start": "nodemon --exec babel-node server.js",
    "server-build": "webpack --config=webpack.server.js",
    "test": "jest",
    "test:coverage": "set CI=true && jest --coverage",
    "build-dev": "webpack --env.NODE_ENV=development",
    "build-prod": "webpack --env.NODE_ENV=production",
    "start-local": "npx webpack-dev-server --env.NODE_ENV=development",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "eslint ./ --cache  --fix",
    "flow": "flow",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-flow": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@cypress/code-coverage": "^1.10.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "chai": "^4.2.0",
    "cross-fetch": "^3.0.4",
    "cypress": "^3.7.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock": "^8.0.1",
    "flow-typed": "^2.6.2",
    "jest": "^24.9.0",
    "jss": "^10.0.0",
    "nodemon": "^2.0.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.9.0",
    "react": "^16.11.0",
    "react-async-ssr": "^0.6.0",
    "react-bootstrap": "^1.0.0-beta.15",
    "react-dom": "^16.11.0",
    "react-jss": "^10.0.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0",
    "redux-persist-immutable": "^4.3.1",
    "redux-saga": "^1.1.3",
    "redux-swagger-client": "^2.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sinon": "^7.5.0",
    "sinon-test": "^2.4.1",
    "storybook-react-router": "^1.0.8",
    "styled-components": "^4.4.1",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "css-loader": "^3.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "flow-bin": "^0.114.0",
    "html-webpack-plugin": "^3.2.0",
    "istanbul-lib-coverage": "^2.0.5",
    "nyc": "^14.1.1",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/*.{js,jsx}",
      "**/src/components/**",
      "**/src/redux/**",
      "!**/src/redux/store.js",
      "!**/src/index.js",
      "!**/src/app.js",
      "!**/src/components/netflixTheme.js"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
